			opp-microvolt-a2 = <0xdbba0>;
			opp-microvolt-a3 = <0xdbba0>;
			opp-microvolt-a5 = <0xe7ef0>;
			clock-latency-ns = <0x3b9b0>;
			opp-supported-hw = <0x2c>;
